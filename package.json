{
  "name": "mf-doom-shop",
  "version": "1.0.0",
  "description": "E-commerce website inspired by MF DOOM",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "npx ts-node scripts/migrate-products.ts",
    "rebuild-products": "npx tsx scripts/analyze-and-create-products.ts",
    "postinstall": "prisma generate",
    "db:docker:up": "docker-compose up -d postgres",
    "db:docker:down": "docker-compose down",
    "db:docker:logs": "docker-compose logs -f postgres",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:export": "tsx scripts/export-supabase-data.ts",
    "db:setup-local": "chmod +x scripts/setup-local-db.sh && ./scripts/setup-local-db.sh",
    "pgadmin": "docker-compose up -d pgadmin"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@paypal/react-paypal-js": "^8.1.3",
    "@prisma/client": "^6.9.0",
    "@types/bcryptjs": "^3.0.0",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "canvas-confetti": "^1.9.2",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "next-auth": "^4.24.11",
    "prisma": "^6.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^3.0.2",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.2.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.6.4",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "instagram-scraping": "^1.0.17",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3"
  }
}
